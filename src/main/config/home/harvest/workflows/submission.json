{
    "harvester": {
        "type": "workflow-harvester",
        "workflow-harvester": {
            "force-storage": "true"
        }
    },

    "transformer": {
        "harvestQueue": [],
        "renderQueue": []
    },

    "indexer": {
        "script": {
            "type": "python",
            "rules": "submission-rules.py"
        },
        "params": {
            "repository.name": "ReDBox",
            "repository.type": "ReDBox",
            "workflow.source": "Submission Request"
        }
    },

    "stages": [
        {
            "name": "inbox",
            "label": "Inbox",
            "description": "New submission requests.",
            "security": ["librarian", "reviewer", "admin"],
            "visibility": ["librarian", "reviewer", "admin"],
            "template": "workflows/submission"
        },
        {
            "name": "live",
            "label": "Processed",
            "description": "Submission requests that have been processed.",
            "security": ["admin"],
            "visibility": ["admin"],
            "template": "workflows/submission"
        }
    ]
}
