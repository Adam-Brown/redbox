#sectionHeading("People")
<table class="meta">
    <tr>
        <th>Creators</th>
        <td>
            #set($creatorList = $self.getList("dc:creator.foaf:Person"))
            #foreach($key in $creatorList.keySet())
                #set($dcCreator = $creatorList.get($key))
                <span>$dcCreator.get("foaf:givenName")&nbsp;$dcCreator.get("foaf:familyName")</span>
                #if($dcCreator.get("pi") == 1), <em>Principal Investigator</em>#end
                #if($dcCreator.get("coPi") == 1), <em>Co-Principal Investigator</em>#end
                #set($association = $dcCreator.get("association.skos:prefLabel"))
                #if($association), <em>$association</em>#end
                <br/>
            #end
        </td>
    </tr>
    <tr>
        <th>Primary Contact</th>
        <td>
            <em>Name</em>: #displayValue("primary_contact_name")
            <em>Email</em>: #displayValue("primary_contact_email")
        </td>
    </tr>
    #display("supervisor" "Supervisor")
    #displayList("collaborators" "Collaborators")
</table>
