#sectionHeading("Subject")
<table class="meta">
    <tr>
        <th>Socio-Economic Objective codes</th>
        <td>
            #set($seoList = $self.getList("dc:subject.anzsrc:seo"))
            #foreach($key in $seoList.keySet())
                #set($code = $seoList.get($key))
                <span>$code.get("skos:prefLabel")</span><br/>
            #end
        </td>
    </tr>
    <tr>
        <th>Field of Research codes</th>
        <td>
            #set($forList = $self.getList("dc:subject.anzsrc:for"))
            #foreach($key in $forList.keySet())
                #set($code = $forList.get($key))
                <span>$code.get("skos:prefLabel")</span><br/>
            #end
        </td>
    </tr>
    #displayList("dc:subject.keywords" "Keywords")
    #display("research_activity" "Research Activity")
</table>
