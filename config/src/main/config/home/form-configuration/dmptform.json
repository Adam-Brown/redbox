                            "multiline":true,
                            "buttons":[
                                {
                                    "label":"Yes, I am responsible for updating information about data in this project.",
                                    "checked":true,
                                    "action":"hide_no_option();"
                                },
                                {
                                    "label":"No. That responsibility has been delegated to someone else.",
                                    "action":"show_no_option();"
                                }
